---
const links = [
  { title: 'Home', url: '/' },
  { title: 'Projects', url: '/projects' },
];
---

<header class="sticky top-0 w-full py-2">
  <nav>
    <ul class="flex gap-2 justify-center">
      {
        links.map((link) => (
          <li>
            <a href={link.url}>{link.title}</a>
          </li>
        ))
      }
    </ul>
  </nav>
</header>
